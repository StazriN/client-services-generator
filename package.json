{
    "name": "client-services-generator",
    "version": "0.9.1",
    "description": "Axios and Angular REST API client generator from OpenAPI v3 (YAML or JSON) or WADL.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "start": "node ./lib/index.js",
        "build_start": "tsc && node ./lib/index.js",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
    },
    "keywords": [],
    "author": "Samuel Obuch",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/StazriN/client-services-generator.git"
    },
    "bin": {
        "client-services-generator": "./lib/index.js"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.1.0",
        "@types/inquirer": "^6.5.0",
        "@types/js-yaml": "^3.12.3",
        "@types/node": "^13.11.1",
        "@types/yargs": "^15.0.4",
        "prettier": "^1.19.1",
        "typescript": "^3.8.3",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2"
    },
    "dependencies": {
        "api-spec-converter": "^2.11.0",
        "fs-extra": "^8.1.0",
        "handlebars": "^4.7.6",
        "inquirer": "^7.1.0",
        "js-yaml": "^3.13.1",
        "xml-js": "^1.6.11",
        "yargs": "^15.3.1"
    },
    "prettier": {
        "trailingComma": "all",
        "arrowParens": "always",
        "singleQuote": true,
        "tabWidth": 4,
        "printWidth": 150
    },
    "eslintConfig": {
        "extends": [
            "plugin:prettier/recommended"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "warn",
                {
                    "trailingComma": "all",
                    "arrowParens": "always",
                    "singleQuote": true,
                    "tabWidth": 4,
                    "printWidth": 150
                }
            ]
        }
    }
}
